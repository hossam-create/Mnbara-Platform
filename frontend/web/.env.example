# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=

# Blockchain Configuration
VITE_BLOCKCHAIN_RPC_URL=http://localhost:8545
VITE_CHAIN_ID=1337

# Feature Flags
VITE_ENABLE_BLOCKCHAIN=false
VITE_ENABLE_ANALYTICS=false
