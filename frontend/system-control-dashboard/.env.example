# System Control Dashboard Environment Variables

# API Configuration
VITE_API_URL=http://localhost:8080
VITE_WS_URL=ws://localhost:8080

# Application Configuration
VITE_APP_TITLE=Mnbara System Control
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_AI_SOLVER=true
VITE_ENABLE_EMERGENCY_MODE=true
VITE_ENABLE_AUTO_PILOT=true

# Monitoring Configuration
VITE_MONITORING_INTERVAL=5000
VITE_ALERT_THRESHOLD=80

# Security Configuration
VITE_SESSION_TIMEOUT=3600000
VITE_ENABLE_AUDIT_LOG=true