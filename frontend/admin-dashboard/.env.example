# MNBARA Admin Dashboard Environment Variables

# API Configuration
VITE_API_URL=http://localhost:8080/api
VITE_ADMIN_API_URL=http://localhost:3012/api

# Sentry Configuration (Error Tracking)
# Get your DSN from https://sentry.io
VITE_SENTRY_DSN=
VITE_ENVIRONMENT=development
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true

# Paymob Integration
VITE_PAYMOB_API_KEY=your_paymob_api_key
VITE_PAYMOB_IFRAME_ID=your_iframe_id
VITE_PAYMOB_INTEGRATION_ID=your_integration_id
